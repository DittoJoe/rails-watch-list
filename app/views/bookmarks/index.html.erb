<h1>All bookmarks</h1>
<ul>
  <% @bookmarks.each do |bookmark| %>
    <li>
      <%= link_to bookmark.title, bookmark_path(bookmark) %>
      <p><%= bookmark.content %></p>
      <%= link_to '(delete)', bookmark_path(bookmark), method: :delete %>
    </li>
  <% end %>
</ul>
<%= link_to 'Add bookmark' , new_bookmark_path  %>
